    "import random\n",
    "n=5\n",
    "random_list=[random.randint(1,100)for _ in range(n)]\n",
    "total_sum=sum(random_list)\n",
    "avg=sum(random_list)/len(random_list)\n",
    "print(\"random list\",random_list)\n",
    "print(\"sum\",total_sum)\n",
    "print(\"average\",avg)"
